<% content_for :body_content_left do %>
  <% @events.each do |event| %>
  	<% if !event.photo.nil? %>
	    <div class="event_image">
	    	<div class="description">
	    		<%= event.name %> <%= event.date.strftime("%Y") %>
	    	</div>
	      <%= link_to(image_fu(event.photo, nil), refinery.events_event_path(event)) %>
	    </div>
  	<% end %>
  <% end %>
<% end %>

<%= render '/refinery/content_page' %>