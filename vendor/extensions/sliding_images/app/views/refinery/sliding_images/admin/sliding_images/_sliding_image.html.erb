<li class='clearfix record <%= cycle("on", "on-hover") %>' id="<%= dom_id(sliding_image) -%>">
  <span class='title'>
    <%= sliding_image.name %>

  </span>
  <span class='actions'>
    
    <%= link_to refinery_icon_tag("application_go.png"), refinery.sliding_images_sliding_image_path(sliding_image),
        :title => t('.view_live_html'),
        :target => "_blank" %>
    
    <%= link_to refinery_icon_tag("application_edit.png"), refinery.edit_sliding_images_admin_sliding_image_path(sliding_image),
         :title => t('.edit') %>
    <%= link_to refinery_icon_tag("delete.png"), refinery.sliding_images_admin_sliding_image_path(sliding_image),
        :class => "cancel confirm-delete",
        :title => t('.delete'),
        :confirm => t('message', :scope => 'refinery.admin.delete', :title => sliding_image.name),
        :method => :delete %>
  </span>
</li>
